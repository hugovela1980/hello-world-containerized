{
  "name": "hello-world-containerized",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "prod_cont": "cross-env NODE_ENV=container powershell -ExecutionPolicy Bypass -File ./run-docker.ps1",
    "start": "node --env-file .env server.js -L -e js,mjs,html,css,toml",
    "dev_cont": "cross-env NODE_ENV=container powershell -ExecutionPolicy Bypass -File ./run-docker.ps1",
    "dev": "nodemon --env-file .env server.js -L -e js,mjs,html,css,toml",
    "local_dev": "cross-env NODE_ENV=development nodemon --env-file .env server.js -L -e js,mjs,html,css,toml",
    "stop": "docker-compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "config": "^3.3.12",
    "delay": "^6.0.0",
    "express": "^4.19.2",
    "pino": "^9.3.2",
    "pino-http": "^10.2.0",
    "pino-pretty": "^11.2.2",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4"
  }
}
